{
  "version": "2.0",
  "app_name": "flask",
  "stages": {
    "dev": {
      "api_gateway_stage": "api"
    },
    "WebApiDev": {
      "api_gateway_stage": "v1",
      "lambda_functions": {
        "api_handler": {
          "manage_iam_role": false,
          "iam_role_arn": "${Token[TOKEN.67]}",
          "environment_variables": {
            "DYNAMODB_TABLE_NAME": "${Token[TOKEN.58]}"
          },
          "lambda_memory_size": 128,
          "lambda_timeout": 10
        }
      }
    }
  }
}